LOCAL_LIBS = 
LIBS = $(LIBRUBYARG_SHARED) -lpthread -lz   
ORIG_SRCS = backtrace.c collector.c compat.c pprof.upb.c pprof_out.c ruby_memprofiler_pprof.c strtab.c
SRCS = $(ORIG_SRCS) decode.c encode.c msg.c table.c upb.c naive.c range2-neon.c range2-sse.c
OBJS = backtrace.o collector.o compat.o pprof.upb.o pprof_out.o ruby_memprofiler_pprof.o strtab.o decode.o encode.o msg.o table.o upb.o naive.o range2-neon.o range2-sse.o
