CXXFLAGS = $(CCDLFLAGS)  $(ARCH_FLAG)
ldflags  = -L. -L/Users/ktsanaktsidis/.rbenv/versions/3.0.4/lib -L/opt/homebrew/lib -fstack-protector-strong
dldflags = -L/Users/ktsanaktsidis/.rbenv/versions/3.0.4/lib -L/opt/homebrew/lib -Wl,-undefined,dynamic_lookup -Wl,-multiply_defined,suppress 
